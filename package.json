{
  "name": "akar-icons",
  "version": "1.9.31",
  "description": "A perfectly rounded icon library made for everyone.",
  "main": "dist/index.js",
  "typings": "dist/icons.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/artcoholic/akar-icons.git",
  "author": {
    "name": "Arturo Wibawa",
    "email": "agwibawa@gmail.com",
    "url": "https://arturowibawa.com"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "inline",
    "figma",
    "akar",
    "design",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/artcoholic/akar-icons/issues"
  },
  "homepage": "https://akaricons.com",
  "scripts": {
    "fetch": "node bin/fetchSVG.js",
    "format:local": "prettier --config package.json --ignore-path .prettierignore --write .",
    "format:ci": "prettier --config package.json --ignore-path .prettierignore --check .",
    "generate": "sudo rm -rf src/icons && node bin/build.js",
    "build:bundle": "sudo rm -rf dist && node_modules/.bin/rollup --c ./build/rollup.config.bundle.js",
    "dev": "rollup -w -c ./build/rollup.config.dev.js",
    "build": "rollup -c ./build/rollup.config.prod.js"
  },
  "config": {
    "style": "stroke"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.77",
    "react-native-svg": "^15.11.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "figma-js": "^1.16.1-0",
    "fs-extra": "^9.0.1",
    "got": "^11.5.1",
    "p-queue": "2.4.2",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.77.1",
    "react-native-svg": "^15.11.2",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^3.3.2",
    "uppercamelcase": "^3.0.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
